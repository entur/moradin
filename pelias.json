{
  "esclient": {
    "hosts": [
      {
        "env": "development",
        "protocol": "http",
        "host": "localhost",
        "port": 9200
      }
    ]
  },
  "api": {
    "targets": {
      "layers_by_source": {
        "nsr": [
          "stop_place",
          "stop_place_child",
          "stop_place_parent",
          "group_of_stop_places"
        ],
        "kartverket": [
          "address",
          "street"
        ],
        "osm": [
          "pointOfInterest"
        ]
      }
    }
  },
  "logger": {
    "level": "debug"
  },
  "imports": {
    "adminLookup": {
      "enabled": false
    },
    "csv": {
      "datapath": "/home/moradin/csv"
    }
  },
  "dbclient": {
    "batchSize": 5000
  }
}